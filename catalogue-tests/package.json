{
    "name": "@platforma-open/milaboratories.software-test-utils",
    "version": "1.1.1",
    "description": "Small cross-platform binaries, like 'sleep' or 'hello-world', suitable for test needs",
    "scripts": {
        "cleanup": "rm -rf ./pkg-*.tgz && rm -rf ./dist/",
        "build": "pl-pkg build"
    },
    "block-software": {
        "entrypoints": {
            "small-asset": {
                "reference": "@platforma-open/milaboratories.software-test-utils.small-asset/dist/tengo/asset/main.as.json"
            },
            "sleep": {
                "reference": "@platforma-open/milaboratories.software-test-utils.sleep/dist/tengo/software/main.sw.json"
            },
            "runenv-java-stub": {
                "reference": "@platforma-open/milaboratories.software-test-utils.runenv-java-stub/dist/tengo/software/main.sw.json"
            },
            "runenv-python-stub": {
                "reference": "@platforma-open/milaboratories.software-test-utils.runenv-python-stub/dist/tengo/software/main.sw.json"
            },
            "hello-world": {
                "reference": "@platforma-open/milaboratories.software-small-binaries.hello-world/dist/tengo/software/main.sw.json"
            },
            "hello-world-py": {
                "reference": "@platforma-open/milaboratories.software-small-binaries.hello-world-py/dist/tengo/software/main.sw.json"
            },
            "guided-command": {
                "reference": "@platforma-open/milaboratories.software-test-utils.guided-command/dist/tengo/software/main.sw.json"
            },
            "stub-python-script": {
                "reference": "@platforma-open/milaboratories.software-test-utils.python-stub/dist/tengo/software/main.sw.json"
            },
            "stub-java-script": {
                "reference": "@platforma-open/milaboratories.software-test-utils.java-stub/dist/tengo/software/main.sw.json"
            },
            "read-file-to-stdout-with-sleep": {
                "reference": "@platforma-open/milaboratories.software-test-utils.read-with-sleep/dist/tengo/software/main.sw.json"
            },
            "docker-only": {
                "reference": "@platforma-open/milaboratories.software-test-utils.docker-images/dist/tengo/software/docker-only.sw.json"
            },
            "docker-true-ep": {
                "reference": "@platforma-open/milaboratories.software-test-utils.docker-images/dist/tengo/software/always-true-ep.sw.json"
            },
            "docker-true-cmd": {
                "reference": "@platforma-open/milaboratories.software-test-utils.docker-images/dist/tengo/software/always-true-cmd.sw.json"
            }
        }
    },
    "files": [
        "dist/"
    ],
    "license": "UNLICENSED",
    "dependencies": {
        "@platforma-open/milaboratories.software-small-binaries.hello-world": "workspace:*",
        "@platforma-open/milaboratories.software-small-binaries.hello-world-py": "workspace:*",
        "@platforma-open/milaboratories.software-test-utils.docker-images": "workspace:*",
        "@platforma-open/milaboratories.software-test-utils.guided-command": "workspace:*",
        "@platforma-open/milaboratories.software-test-utils.java-stub": "workspace:*",
        "@platforma-open/milaboratories.software-test-utils.python-stub": "workspace:*",
        "@platforma-open/milaboratories.software-test-utils.read-with-sleep": "workspace:*",
        "@platforma-open/milaboratories.software-test-utils.runenv-java-stub": "workspace:*",
        "@platforma-open/milaboratories.software-test-utils.runenv-python-stub": "workspace:*",
        "@platforma-open/milaboratories.software-test-utils.sleep": "workspace:*",
        "@platforma-open/milaboratories.software-test-utils.small-asset": "workspace:*"
    },
    "devDependencies": {
        "@platforma-sdk/package-builder": "catalog:"
    }
}
